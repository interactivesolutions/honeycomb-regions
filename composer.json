{
    "name": "interactivesolutions/honeycomb-regions",
    "description": "HoneyComb regions package. World continents, countries, cities, municipalities, city parts, streets. Availability to read, create and update.",
    "keywords": ["HoneyComb", "package", "world", "regions", "continents", "cities", "municipalities", "city parts", "streets"],
    "homepage": "https://github.com/interactivesolutions/honeycomb-regions/",
    "license": "MIT",
    "authors": [
        {
            "name": "Jevgenij Dmitrijev",
            "email": "jd@interactivesolutions.lt"
        },
        {
            "name": "Mantas Gelčius",
            "email": "mantas.gelcius@gmail.com"
        },
        {
            "name": "Liutauras Kavaliauskas",
            "email": "liutauraskav@gmail.com"
        },
        {
            "name": "Vytautas Rimeikis",
            "email": "vytautas.rimeikis@gmail.com"
        }
    ],
    "repositories": [],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.5",
        "rinvex/country": "^3.1",
        "interactivesolutions/honeycomb-core": "^1.1.0",
        "interactivesolutions/honeycomb-languages": "^1.1"
    },
    "require-dev": {
        "brianium/paratest": "^1.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench-browser-kit": "^3.5",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "interactivesolutions\\honeycombregions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "paratest -p4 --colors"
        ]
    },
    "config": {
        "secure-http": false,
        "platform": {
            "php": "7.1"
        }
    }
}
